coroutine.wrap(OOQOWWP_fake_script)()
local function FOHCLAP_fake_script() -- DGNZ.LocalScript 
	local script = Instance.new('LocalScript', DGNZ)

	-- Player Orbit Script
	-- Press B to toggle orbiting around your target
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local LocalPlayer = Players.LocalPlayer
	local Mouse = LocalPlayer:GetMouse()
	local Camera = workspace.CurrentCamera
	
	-- Configuration
	local Settings = {
		OrbitKey = Enum.KeyCode.B,
		OrbitSpeed = 5,
		OrbitRadius = 10,
		OrbitHeight = 0,
		MaxDistance = 500,
		CheckRate = 0.1
	}
	
	-- Variables
	local Target = nil
	local IsOrbiting = false
	local Connection = nil
	local OrbitAngle = 0
	
	-- UI Components (Inside DGNZ+ ScreenGui)
	local ScreenGui = game.Players.LocalPlayer.PlayerGui:WaitForChild("DGNZ+")
	
	-- Reference to the Orbit button
	local OrbitButton = ScreenGui:WaitForChild("Frame"):WaitForChild("Orbit")
	
	-- Target Frame
	local TargetFrame = Instance.new("Frame")
	TargetFrame.Name = "TargetFrame"
	TargetFrame.Size = UDim2.new(0, 200, 0, 70)
	TargetFrame.Position = UDim2.new(0, 10, 0, 10)
	TargetFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	TargetFrame.BackgroundTransparency = 0.3
	TargetFrame.BorderSizePixel = 0
	TargetFrame.Visible = false
	TargetFrame.Parent = ScreenGui
	
	-- Target Avatar
	local TargetAvatar = Instance.new("ImageLabel")
	TargetAvatar.Name = "TargetAvatar"
	TargetAvatar.Size = UDim2.new(0, 60, 0, 60)
	TargetAvatar.Position = UDim2.new(0, 5, 0, 5)
	TargetAvatar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TargetAvatar.BackgroundTransparency = 1
	TargetAvatar.BorderSizePixel = 0
	TargetAvatar.Parent = TargetFrame
	
	-- Target Label
	local TargetLabel = Instance.new("TextLabel")
	TargetLabel.Name = "TargetLabel"
	TargetLabel.Size = UDim2.new(0, 120, 0, 30)
	TargetLabel.Position = UDim2.new(0, 70, 0, 5)
	TargetLabel.BackgroundTransparency = 1
	TargetLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TargetLabel.TextSize = 18
	TargetLabel.Font = Enum.Font.SourceSansBold
	TargetLabel.Text = "Target Found!"
	TargetLabel.TextXAlignment = Enum.TextXAlignment.Left
	TargetLabel.Parent = TargetFrame
	
	-- Target Name
	local TargetName = Instance.new("TextLabel")
	TargetName.Name = "TargetName"
	TargetName.Size = UDim2.new(0, 120, 0, 20)
	TargetName.Position = UDim2.new(0, 70, 0, 35)
	TargetName.BackgroundTransparency = 1
	TargetName.TextColor3 = Color3.fromRGB(200, 200, 200)
	TargetName.TextSize = 14
	TargetName.Font = Enum.Font.SourceSans
	TargetName.Text = ""
	TargetName.TextXAlignment = Enum.TextXAlignment.Left
	TargetName.Parent = TargetFrame
	
	-- Functions
	local function GetClosestPlayerToMouse()
		local MaxDist = Settings.MaxDistance
		local Closest = nil
	
		for _, Player in pairs(Players:GetPlayers()) do
			if Player ~= LocalPlayer then
				local Character = Player.Character
				if Character and Character:FindFirstChild("HumanoidRootPart") and Character:FindFirstChild("Humanoid") and Character.Humanoid.Health > 0 then
					local HRP = Character.HumanoidRootPart
					local ScreenPoint = Camera:WorldToScreenPoint(HRP.Position)
					local DistFromMouse = (Vector2.new(ScreenPoint.X, ScreenPoint.Y) - Vector2.new(Mouse.X, Mouse.Y)).Magnitude
	
					if ScreenPoint.Z > 0 and DistFromMouse < MaxDist then
						MaxDist = DistFromMouse
						Closest = Player
					end
				end
			end
		end
	
		return Closest
	end
	
	local function UpdateTargetUI(Player)
		if Player then
			local AvatarUrl = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. Player.UserId .. "&width=420&height=420&format=png"
			TargetAvatar.Image = AvatarUrl
			TargetName.Text = Player.Name
			TargetFrame.Visible = true
	
			-- Animate target frame appearance
			TargetFrame.Position = UDim2.new(0, -200, 0, 10)
			TweenService:Create(TargetFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Position = UDim2.new(0, 10, 0, 10)}):Play()
		else
			TargetFrame.Visible = false
		end
	end
	
	local function StartOrbiting()
		if Connection then
			Connection:Disconnect()
			Connection = nil
		end
	
		IsOrbiting = true
		print("Started Orbiting")  -- Debugging
	
		Connection = RunService.RenderStepped:Connect(function(dt)
			if not Target or not Target.Character or not Target.Character:FindFirstChild("HumanoidRootPart") then
				print("Target is invalid, finding new target")  -- Debugging
				Target = GetClosestPlayerToMouse()
				if Target then
					UpdateTargetUI(Target)
				else
					IsOrbiting = false
					Connection:Disconnect()
					Connection = nil
					UpdateTargetUI(nil)
					return
				end
			end
	
			local HRP = Target.Character.HumanoidRootPart
			OrbitAngle = OrbitAngle + dt * Settings.OrbitSpeed
	
			local X = HRP.Position.X + math.cos(OrbitAngle) * Settings.OrbitRadius
			local Z = HRP.Position.Z + math.sin(OrbitAngle) * Settings.OrbitRadius
			local Y = HRP.Position.Y + Settings.OrbitHeight
	
			LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(X, Y, Z, 0, 0, 0, 0, 0, 0)
			LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.lookAt(LocalPlayer.Character.HumanoidRootPart.Position, HRP.Position)
		end)
	end
	
	local function StopOrbiting()
		IsOrbiting = false
		if Connection then
			Connection:Disconnect()
			Connection = nil
		end
		UpdateTargetUI(nil)
	end
	
	-- Button click handler
	OrbitButton.MouseButton1Click:Connect(function()
		print("Button Clicked!")  -- For debugging
		if not IsOrbiting then
			Target = GetClosestPlayerToMouse()
			if Target then
				UpdateTargetUI(Target)
				StartOrbiting()
			else
				print("No target found")  -- Debugging
			end
		else
			StopOrbiting()
		end
	end)
	
	-- Script loaded confirmation
	print("Player Orbit Script loaded. Click the Orbit button to activate.")
	
end
coroutine.wrap(FOHCLAP_fake_script)()
