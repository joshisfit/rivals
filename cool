coroutine.wrap(RGNXEBC_fake_script)()
local function XHVUK_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	-- Extremely Basic Aimbot for Baseplate
	-- This must be inserted as a LocalScript in StarterPlayerScripts
	
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")
	
	-- Print immediately to confirm script is running
	print("AIMBOT SCRIPT STARTING")
	
	-- Variables
	local player = Players.LocalPlayer
	local camera = workspace.CurrentCamera
	local enabled = false
	
	-- Toggle function
	local function toggle()
		enabled = not enabled
		print("Aimbot: " .. (enabled and "ENABLED" or "DISABLED"))
	end
	
	-- Connect Q key to toggle
	UserInputService.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.Q then
			toggle()
		end
	end)
	
	-- Create a part to verify script is working
	local function createIndicator()
		local part = Instance.new("Part")
		part.Anchored = true
		part.CanCollide = false
		part.Size = Vector3.new(1, 1, 1)
		part.Color = Color3.new(1, 0, 0)
		part.Position = workspace.CurrentCamera.CFrame.Position + Vector3.new(0, 5, 0)
		part.Parent = workspace
	
		-- Destroy after 5 seconds
		task.delay(5, function()
			part:Destroy()
		end)
	end
	
	-- Try to create indicator to verify script is running
	pcall(createIndicator)
	
	-- Main aimbot loop
	RunService.RenderStepped:Connect(function()
		if not enabled then return end
	
		-- Find closest player
		local closest = nil
		local closestDistance = math.huge
	
		for _, otherPlayer in pairs(Players:GetPlayers()) do
			if otherPlayer ~= player then
				local character = otherPlayer.Character
				if character and character:FindFirstChild("Head") then
					local distance = (character.Head.Position - camera.CFrame.Position).Magnitude
					if distance < closestDistance then
						closestDistance = distance
						closest = character.Head
					end
				end
			end
		end
	
		-- Aim at closest player
		if closest then
			-- Direct camera manipulation
			workspace.CurrentCamera.CFrame = CFrame.new(camera.CFrame.Position, closest.Position)
		end
	end)
	
	-- Print to confirm full script loaded
	print("AIMBOT SCRIPT LOADED - PRESS Q TO TOGGLE")
end
coroutine.wrap(XHVUK_fake_script)()
