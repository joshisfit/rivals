coroutine.wrap(NJSZIVU_fake_script)()
local function BUSS_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	-- CFrame Speed Script
	-- Place this script directly inside an existing ImageButton
	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	local player = Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")
	local rootPart = character:WaitForChild("HumanoidRootPart")
	
	-- Reference to the button this script is in
	local speedButton = script.Parent
	
	-- Speed variables
	local usingCFrameSpeed = false
	local speedMultiplier = 2  -- Adjust this value to change the speed
	
	-- Function to toggle CFrame speed
	local function toggleCFrameSpeed()
		usingCFrameSpeed = not usingCFrameSpeed
	end
	
	-- Connect button click
	speedButton.MouseButton1Click:Connect(toggleCFrameSpeed)
	
	-- Connect C key press
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if not gameProcessed and input.KeyCode == Enum.KeyCode.C then
			toggleCFrameSpeed()
		end
	end)
	
	-- Handle CFrame speed movement
	RunService.RenderStepped:Connect(function(delta)
		if usingCFrameSpeed and character and humanoid and rootPart and humanoid.Health > 0 then
			local camera = workspace.CurrentCamera
			local lookVector = camera.CFrame.LookVector
			local rightVector = camera.CFrame.RightVector
	
			-- Get the normalized direction
			local moveDirection = Vector3.new()
	
			-- WASD Controls
			if UserInputService:IsKeyDown(Enum.KeyCode.W) then
				moveDirection = moveDirection + lookVector
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.S) then
				moveDirection = moveDirection - lookVector
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.A) then
				moveDirection = moveDirection - rightVector
			end
			if UserInputService:IsKeyDown(Enum.KeyCode.D) then
				moveDirection = moveDirection + rightVector
			end
	
			-- Remove Y component to keep movement horizontal
			moveDirection = Vector3.new(moveDirection.X, 0, moveDirection.Z)
	
			-- Apply speed if we're moving
			if moveDirection.Magnitude > 0.1 then
				-- Normalize the direction and apply the speed multiplier
				moveDirection = moveDirection.Unit
	
				-- CFrame manipulation for speed
				rootPart.CFrame = rootPart.CFrame + (moveDirection * speedMultiplier)
			end
		end
	end)
	
	-- Handle character respawning
	player.CharacterAdded:Connect(function(newCharacter)
		character = newCharacter
		humanoid = character:WaitForChild("Humanoid")
		rootPart = character:WaitForChild("HumanoidRootPart")
	
		-- Reset CFrame speed state when respawning
		usingCFrameSpeed = false
	end)
end
coroutine.wrap(BUSS_fake_script)()
