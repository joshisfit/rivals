coroutine.wrap(KARNMY_fake_script)()
local function CVSYZI_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	-- LocalScript inside your Frame
	local frame = script.Parent
	local userInputService = game:GetService("UserInputService")
	local tweenService = game:GetService("TweenService")
	local runService = game:GetService("RunService")
	
	local dragging = false
	local offset = Vector2.new()
	local followSpeed = 0.2
	local visible = true
	local fadeTime = 0.3
	
	-- Store all UI elements + their original transparencies
	local uiElements = {}
	
	local function collectUIElements()
		for _, child in pairs(frame:GetDescendants()) do
			if child:IsA("TextLabel") or child:IsA("TextButton") then
				table.insert(uiElements, {
					instance = child,
					properties = {
						TextTransparency = child.TextTransparency,
						BackgroundTransparency = child.BackgroundTransparency
					}
				})
			elseif child:IsA("ImageLabel") or child:IsA("ImageButton") then
				table.insert(uiElements, {
					instance = child,
					properties = {
						ImageTransparency = child.ImageTransparency,
						BackgroundTransparency = child.BackgroundTransparency
					}
				})
			elseif child:IsA("UIStroke") then
				table.insert(uiElements, {
					instance = child,
					properties = {
						Transparency = child.Transparency
					}
				})
			end
		end
	
		-- Save the frame's background transparency too
		table.insert(uiElements, {
			instance = frame,
			properties = {
				BackgroundTransparency = frame.BackgroundTransparency
			}
		})
	end
	
	collectUIElements()
	
	-- Smooth drag logic
	local targetPos = frame.Position
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			local mousePos = userInputService:GetMouseLocation()
			offset = Vector2.new(mousePos.X - frame.AbsolutePosition.X, mousePos.Y - frame.AbsolutePosition.Y)
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	runService.RenderStepped:Connect(function()
		if dragging then
			local mousePos = userInputService:GetMouseLocation()
			local newPos = UDim2.new(0, mousePos.X - offset.X, 0, mousePos.Y - offset.Y)
			targetPos = newPos
		end
	
		local current = frame.Position
		frame.Position = UDim2.new(
			0,
			current.X.Offset + (targetPos.X.Offset - current.X.Offset) * followSpeed,
			0,
			current.Y.Offset + (targetPos.Y.Offset - current.Y.Offset) * followSpeed
		)
	end)
	
	-- Fade toggle
	local function fadeFrame(show)
		if show then
			frame.Visible = true
		end
	
		for _, item in pairs(uiElements) do
			local instance = item.instance
			local original = item.properties
			local goal = {}
	
			for prop, value in pairs(original) do
				goal[prop] = show and value or 1
			end
	
			local tween = tweenService:Create(instance, TweenInfo.new(fadeTime), goal)
			tween:Play()
		end
	
		if not show then
			task.delay(fadeTime, function()
				frame.Visible = false
			end)
		end
	end
	
	-- Keybind to toggle with M
	userInputService.InputBegan:Connect(function(input, processed)
		if processed then return end
		if input.KeyCode == Enum.KeyCode.M then
			visible = not visible
			fadeFrame(visible)
		end
	end)
	
end
coroutine.wrap(CVSYZI_fake_script)()
