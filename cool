local function GSAXKG_fake_script() -- TextButton_10.LocalScript 
	local script = Instance.new('LocalScript', TextButton_10)

	local button = script.Parent
	local players = game:GetService("Players")
	local localPlayer = players.LocalPlayer
	local runService = game:GetService("RunService")
	
	local enabled = false
	local bars = {}
	
	local function createBar(player)
		if player == localPlayer then return end
		if bars[player] then return end
	
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
		local hum = char:WaitForChild("Humanoid")
	
		local gui = Instance.new("BillboardGui")
		gui.Name = "HealthESP"
		gui.Adornee = hrp
		gui.Size = UDim2.new(0, 6, 0, 120) -- Thin bar, tall height
		gui.AlwaysOnTop = true
		gui.ExtentsOffset = Vector3.new(-2.5, 3, 0) -- Offset left side of body
	
		local bg = Instance.new("Frame")
		bg.Size = UDim2.new(1, 0, 1, 0)
		bg.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		bg.BorderSizePixel = 0
		bg.Parent = gui
	
		local fill = Instance.new("Frame")
		fill.AnchorPoint = Vector2.new(0, 1)
		fill.Position = UDim2.new(0, 0, 1, 0)
		fill.Size = UDim2.new(1, 0, 1, 0)
		fill.BackgroundColor3 = Color3.new(1, 1, 1)
		fill.BorderSizePixel = 0
		fill.Parent = bg
	
		local hpText = Instance.new("TextLabel")
		hpText.Position = UDim2.new(0, 0, 0, -12)
		hpText.Size = UDim2.new(1, 0, 0, 10)
		hpText.Font = Enum.Font.Arcade or Enum.Font.Legacy
		hpText.TextColor3 = Color3.new(1, 1, 1)
		hpText.TextStrokeColor3 = Color3.new(0, 0, 0)
		hpText.TextStrokeTransparency = 0
		hpText.TextSize = 10
		hpText.BackgroundTransparency = 1
		hpText.Text = "100"
		hpText.Parent = bg
	
		gui.Parent = workspace.CurrentCamera
	
		bars[player] = {
			gui = gui,
			humanoid = hum,
			fill = fill,
			text = hpText
		}
	end
	
	local function removeBar(player)
		if bars[player] then
			bars[player].gui:Destroy()
			bars[player] = nil
		end
	end
	
	players.PlayerAdded:Connect(createBar)
	players.PlayerRemoving:Connect(removeBar)
	for _, p in pairs(players:GetPlayers()) do createBar(p) end
	
	runService.RenderStepped:Connect(function()
		if not enabled then
			for _, data in pairs(bars) do
				data.gui.Enabled = false
			end
			return
		end
	
		for _, data in pairs(bars) do
			local hum = data.humanoid
			if hum and hum.Health > 0 then
				local hpPerc = math.clamp(hum.Health / hum.MaxHealth, 0, 1)
				data.fill.Size = UDim2.new(1, 0, hpPerc, 0)
				data.text.Text = tostring(math.floor(hum.Health))
				data.gui.Enabled = true
			else
				data.gui.Enabled = false
			end
		end
	end)
	
	button.MouseButton1Click:Connect(function()
		enabled = not enabled
	end)
	
end
