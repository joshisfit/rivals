-- Scripts:

local function TTEQ_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local imageButton = script.Parent
	
	-- Get the player's userId
	local userId = player.UserId
	
	-- Set the ImageButton to their avatar's headshot
	imageButton.Image = string.format("https://www.roblox.com/headshot-thumbnail/image?userId=%d&width=420&height=420&format=png", userId)
	
end
coroutine.wrap(TTEQ_fake_script)()
local function DBPVP_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local TweenService = game:GetService("TweenService")
	
	local originalSize = button.Size
	local shrinkSize = UDim2.new(originalSize.X.Scale * 0.92, 0, originalSize.Y.Scale * 0.92, 0)
	
	local shrinkTween = TweenService:Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = shrinkSize})
	local growTween = TweenService:Create(button, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = originalSize})
	
	button.MouseEnter:Connect(function()
		shrinkTween:Play()
	end)
	
	button.MouseLeave:Connect(function()
		growTween:Play()
	end)
	
end
coroutine.wrap(DBPVP_fake_script)()
